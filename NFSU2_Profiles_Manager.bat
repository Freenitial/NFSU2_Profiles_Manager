# Main processing logic encapsulated for both silent and GUI modes.
function Format-Profile {param([string] $SelectedPseudo, [string] $NewPseudo, [bool] $CloneMode = $false)
         Write-Message "Starting processing: $SelectedPseudo -> $NewPseudo (Clone: $CloneMode)"
    $selectedProfileDir  = Join-Path $NFSlocalAppData $SelectedPseudo
    $selectedProfileFile = Join-Path $selectedProfileDir $SelectedPseudo
    $newProfileDir       = Join-Path $NFSlocalAppData $NewPseudo
    $newProfileFile      = Join-Path $newProfileDir $NewPseudo
    if (Test-Path $selectedProfileDir) {
        if ($CloneMode) {
            Write-Message "Cloning profile '$SelectedPseudo' -> '$NewPseudo'."
            if (-not (Test-Path $newProfileDir)) {
                New-Item -ItemType Directory -Path $newProfileDir -Force | Out-Null
                Copy-Item -LiteralPath $selectedProfileFile -Destination $newProfileFile -Force
                Write-Message "Clone completed."
            } else { Write-Message "Profile '$NewPseudo' already exists." ; throw "Profile '$NewPseudo' already exists." }
        } else { Write-Message "Modifying profile '$SelectedPseudo' -> '$NewPseudo'." }
    } else {
        Write-Message "Profile '$SelectedPseudo' not found. Creating new from template."
        $base64Template = @"

"@
        $templateBytes = [Convert]::FromBase64String($base64Template)
        if (-not (Test-Path $selectedProfileDir)) {
            New-Item -ItemType Directory -Path $selectedProfileDir -Force | Out-Null
            Write-Message "Created profile directory."
        }
        [IO.File]::WriteAllBytes($selectedProfileFile, $templateBytes)
        Write-Message "Wrote embedded template."
        $selectedPseudo = "default"
    }
    # Rename folder and file if needed
    if ($selectedProfileFile -ne $newProfileFile -and -not $clone) {
        try {
            Rename-Item -LiteralPath $selectedProfileFile -NewName $NewPseudo -Force
            Write-Message "Renamed profile file to: '$NewPseudo'."
        } catch { Write-Message "Error while renaming profile file '$selectedProfileFile' into '$NewPseudo' : $_"}
        try {
            Rename-Item -LiteralPath $selectedProfileDir -NewName $NewPseudo -Force
            Write-Message "Renamed profile folder to: '$NewPseudo'."
        } catch { Write-Message "Error while renaming profile folder '$selectedProfileDir' into '$NewPseudo' : $_"}
    } else { Write-Message "Skipping rename since name is unchanged: '$SelectedPseudo'." }
    # Read and patch binary file
    Write-Message "Reading bytes from profile file: $newProfileFile."
    $bytes = [System.IO.File]::ReadAllBytes($newProfileFile)
    Write-Message "Read: '$($bytes.Length)' bytes."
    $currentMarkerBytes = [System.Text.Encoding]::ASCII.GetBytes($SelectedPseudo)
    $offset = -1
    for ($i = 0; $i -le $bytes.Length - $currentMarkerBytes.Length; $i++) {
        $match = $true
        for ($j = 0; $j -lt $currentMarkerBytes.Length; $j++) { if ($bytes[$i + $j] -ne $currentMarkerBytes[$j]) { $match = $false ; break } }
        if ($match) { $offset = $i ; break }
    }
    if ($offset -eq -1) { Write-Message "Marker '$SelectedPseudo' not found in profile file." ; throw "Marker '$SelectedPseudo' not found in profile file." }
    else { Write-Message "Found marker '$SelectedPseudo' at offset $offset." }
    # Build the new pseudo block (7 bytes, padded with null bytes if necessary).
    Write-Message "Creating new pseudo '$NewPseudo' block."
    $newPseudoBytesRaw = [System.Text.Encoding]::ASCII.GetBytes($NewPseudo)
    $newBlock = New-Object byte[] 7
    for ($i = 0; $i -lt 7; $i++) {
        if ($i -lt $newPseudoBytesRaw.Length) { $newBlock[$i] = $newPseudoBytesRaw[$i] }
        else { $newBlock[$i] = 0x00 }
    }
    Write-Message "Replacing marker in profile file with new pseudo block."
    for ($i = 0; $i -lt 7; $i++) { $bytes[$offset + $i] = $newBlock[$i] }
    Write-Message "Overwrite the profile file with the modified binary content."
    [System.IO.File]::WriteAllBytes($newProfileFile, $bytes)
    Write-Message "Processing completed successfully."
}

function Update-existingPseudos {
    $dirs = Get-ChildItem -LiteralPath $NFSlocalAppData -Directory | Where-Object { Test-Path (Join-Path $_.FullName $_.Name) }
    $dirs | ForEach-Object {
        $parent = $_
        Get-ChildItem -LiteralPath $parent.FullName -File |
            Where-Object { $_.BaseName -eq $parent.Name -and $_.Extension -eq '' } |
            Select-Object -ExpandProperty BaseName
    }
}

function Update-ListBoxProfiles {
    $listBox.Items.Clear()
    if ($script:existingPseudos.Count -gt 0) {
        if ($tableLayout.Controls.Contains($listBox_overlay)) { $tableLayout.Controls.Remove($listBox_overlay) }
        $listBox.Enabled    = $true
        $rbModify.Enabled   = $true
        $rbClone.Enabled    = $true
        $rbDelete.Enabled   = $true
        $listBox.ForeColor  = $fgColor
        $listBox.BackColor  = $editBgColor
        $listBox.Items.AddRange($script:existingPseudos)
        $rbModify.Checked   = $true
        if ($script:existingPseudos.Count -eq 1) { $listBox.SelectedIndex = 0 }
    } else {
        $rbCreate.Checked   = $true
        $listBox.Enabled    = $false
        $rbModify.Enabled   = $false
        $rbClone.Enabled    = $false
        $rbDelete.Enabled   = $false
        $listBox.BackColor  = $bgColorDisabled
        $listBox.ForeColor  = $bgColorDisabled
        if (-not $tableLayout.Controls.Contains($listBox_overlay)) { $tableLayout.Controls.Add($listBox_overlay, 0, 2) }
    }
    $textBox.Clear()
}

function Start-ProfileAction {
    if ($rbCreate.Checked) {
        $selected = $textBox.Text; $new = $textBox.Text; $clone = $false
        if (( $listBox.Items | ForEach-Object { $_.ToString().ToLower() } ) -contains $new.ToLower()) { [System.Windows.Forms.MessageBox]::Show("Please choose another name.","Warning"); return }
    }
    elseif ($rbModify.Checked) {
        if (-not $listBox.SelectedItem) { [System.Windows.Forms.MessageBox]::Show("Please select a profile to modify.","Error"); return }
        $selected = $listBox.SelectedItem.ToString(); $new = $textBox.Text; $clone = $false
        if (( $listBox.Items | ForEach-Object { $_.ToString().ToLower() } ) -contains $new.ToLower()) { [System.Windows.Forms.MessageBox]::Show("Please choose another name.","Warning"); return }
    }
    elseif ($rbClone.Checked) {
        if (-not $listBox.SelectedItem) { [System.Windows.Forms.MessageBox]::Show("Please select a profile to clone.","Error"); return }
        $selected = $listBox.SelectedItem.ToString(); $new = $textBox.Text; $clone = $true
        if (( $listBox.Items | ForEach-Object { $_.ToString().ToLower() } ) -contains $new.ToLower()) { [System.Windows.Forms.MessageBox]::Show("Please choose another name.","Warning"); return }
    }
    elseif ($rbDelete.Checked) {
        if (-not $listBox.SelectedItem) { [System.Windows.Forms.MessageBox]::Show("Please select a profile to delete.","Error"); return }
        $selected = $listBox.SelectedItem.ToString()
        $first = [System.Windows.Forms.MessageBox]::Show("Are you sure you want to delete profile '$selected'?","Confirm Delete",[System.Windows.Forms.MessageBoxButtons]::YesNo,[System.Windows.Forms.MessageBoxIcon]::Warning); if ($first -ne [System.Windows.Forms.DialogResult]::Yes) { return }
        $second = [System.Windows.Forms.MessageBox]::Show("This makes Rachel angry. Delete '$selected' for real?","Confirm Deletion Again",[System.Windows.Forms.MessageBoxButtons]::YesNo,[System.Windows.Forms.MessageBoxIcon]::Warning); if ($second -ne [System.Windows.Forms.DialogResult]::Yes) { return }
        $removePath = Join-Path $NFSlocalAppData $selected; Remove-Item -LiteralPath $removePath -Recurse -Force; return
    }
    Format-Profile -SelectedPseudo $selected -NewPseudo $new -CloneMode $clone
}

