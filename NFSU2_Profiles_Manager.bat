foreach ($rb in @($rbCreate, $rbModify, $rbClone,$rbDelete)) {
    $setStyle = $rb.GetType().GetMethod("SetStyle",[Reflection.BindingFlags]"Instance,NonPublic")
    $styles   = [System.Windows.Forms.ControlStyles]::UserPaint -bor [System.Windows.Forms.ControlStyles]::AllPaintingInWmPaint -bor [System.Windows.Forms.ControlStyles]::OptimizedDoubleBuffer
    $setStyle.Invoke($rb, @($styles, $true))
    $rb.BackColor = $bgColor
    $rb.Add_Paint({
        param($s, $e)
        $col = if ($s.Enabled) { [System.Drawing.Color]::White } else { $fgColorDisabled }
        $e.Graphics.Clear($s.BackColor)
        $d=14; $y=($s.Height - $d)/2; $circle=[System.Drawing.Rectangle]::new(0,$y,$d,$d)
        $e.Graphics.DrawEllipse([System.Drawing.Pen]::new([System.Drawing.Color]::White), $circle)
        if ($s.Checked) { $inner = [System.Drawing.Rectangle]::new($circle.X+4, $circle.Y+4, $d-8, $d-8) ; $e.Graphics.FillEllipse([System.Drawing.SolidBrush]::new($fgColor), $inner) }
        $tx = $circle.Right+4; $ty=($s.Height - $s.Font.Height)/2
        $e.Graphics.DrawString($s.Text, $s.Font, [System.Drawing.SolidBrush]::new($col), $tx, $ty)
    })
    $rb.Invalidate()
    $rb.Margin = [System.Windows.Forms.Padding]::new(9, 5, 9, 5)
}

